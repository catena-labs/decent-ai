# App Environment Configuration
# development, production, or test
EXPO_PUBLIC_APP_ENV=

# App Variant
# development, preview, staging, or production
EXPO_PUBLIC_APP_VARIANT=

# API Configuration
# Base URL for the API endpoints
EXPO_PUBLIC_API_URL=

# WalletConnect Configuration
# Project ID from WalletConnect Cloud
EXPO_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Google Authentication
# iOS Client ID for Google Sign-In
EXPO_PUBLIC_AUTH_GOOGLE_IOS_ID=
# Web Client ID for Google Sign-In
EXPO_PUBLIC_AUTH_GOOGLE_WEB_ID=

# Analytics and Monitoring
# PostHog API Key for analytics (optional)
EXPO_PUBLIC_POSTHOG_API_KEY=
# Sentry DSN for error tracking (optional)
EXPO_PUBLIC_SENTRY_DSN=

# RevenueCat Configuration
# API Key for iOS in-app purchases
EXPO_PUBLIC_REVENUECAT_APPLE_API_KEY=
# API Key for Google Play in-app purchases
EXPO_PUBLIC_REVENUECAT_GOOGLE_API_KEY=
